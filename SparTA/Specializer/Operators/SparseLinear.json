
{
    "name": "sparse_matmul",
    "dims": [
        "GLOBAL_M_VALUE",
        "GLOBAL_N_VALUE",
        "GLOBAL_K_VALUE",
        "BLOCK_SIZE_M_VALUE",
        "BLOCK_SIZE_N_VALUE",
        "BLOCK_SIZE_K_VALUE",
        "THREAD_SIZE_M_VALUE",
        "THREAD_SIZE_N_VALUE",
        "THREAD_SIZE_K_VALUE"
    ],
    "inputs": {
        "A": {"type": "float", "shape": ["GLOBAL_M_VALUE", "GLOBAL_K_VALUE"], "layout": "dense"},
        "W": {"type": "float", "shape": ["GLOBAL_N_VALUE", "GLOBAL_K_VALUE"], "layout": "bcsr"},
        "bias": {"type": "float", "shape": ["GLOBAL_N_VALUE"], "layout": "dense"}
    },
    "outputs": {
        "C": {
            "type": "float",
            "shape": ["GLOBAL_M_VALUE", "GLOBAL_N_VALUE"],
            "layout": "dense",
            "formula": "A @ W.T + bias"
        }
    },
    "tiles": {
        "block": ["BLOCK_SIZE_N_VALUE // THREAD_SIZE_N_VALUE", "BLOCK_SIZE_M_VALUE // THREAD_SIZE_M_VALUE"],
        "grid": ["GLOBAL_N_VALUE // BLOCK_SIZE_N_VALUE", "GLOBAL_M_VALUE // BLOCK_SIZE_M_VALUE"]
    }
}
