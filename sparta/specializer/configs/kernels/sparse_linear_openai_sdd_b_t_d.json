{
    "op_name": "sparse_matmul_sdd",
    "factory_type": "template",
    "kernel_name": "sparse_matmul_openai_sdd",
    "dynamic_dims": [
        "GLOBAL_M_VALUE",
        "GLOBAL_N_VALUE",
        "GLOBAL_K_VALUE"
    ],
    "fixed_dims": {
        "BIASED": true,
        "TRANSPOSE": true,
        "COMPRESSED": false
    },
    "inputs": {
        "A": {
            "type": "float",
            "shape": ["GLOBAL_M_VALUE", "GLOBAL_K_VALUE"],
            "layout": "bcsr",
            "mode": "HD",
            "block_size": [32, 64]
        },
        "B": {
            "type": "float",
            "shape": ["GLOBAL_N_VALUE", "GLOBAL_K_VALUE"],
            "layout": "dense"
        },
        "bias": {
            "type":"float",
            "shape": ["GLOBAL_N_VALUE"],
            "layout": "dense"
        }
    },
    "outputs": {
        "C": {
            "type": "float",
            "shape": ["GLOBAL_M_VALUE", "GLOBAL_N_VALUE"],
            "layout": "dense",
            "formula": "A @ B.T + bias"
        }
    },
    "tiles": {
        "block": [256],
        "grid": ["GLOBAL_N_VALUE // 32", "GLOBAL_M_VALUE // 32"]
    }
}
